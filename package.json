{
  "name": "tiny-bilibili-ws",
  "type": "module",
  "version": "0.0.1",
  "packageManager": "pnpm@7.5.2",
  "description": "",
  "author": "starknt",
  "license": "MIT",
  "homepage": "https://github.com/starknt/tiny-bilibili-ws#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/starknt/tiny-bilibili-ws.git"
  },
  "bugs": "https://github.com/starknt/tiny-bilibili-ws/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./browser": {
      "types": "./dist/browser.d.ts",
      "require": "./dist/browser.cjs",
      "import": "./dist/browser.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "browser.d.ts"
  ],
  "scripts": {
    "build": "unbuild",
    "prepublishOnly": "pnpm run build",
    "release": "bumpp && pnpm publish",
    "start": "esno ./src/node.ts",
    "dev": "unbuild --stub",
    "typecheck": "npx tsc --noEmit",
    "test": "vitest",
    "lint": "eslint ."
  },
  "dependencies": {
    "eventemitter3": "^4.0.7",
    "isomorphic-ws": "^5.0.0",
    "pako": "^2.0.4",
    "ws": "^8.9.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.25.2",
    "@types/node": "^18.0.6",
    "@types/pako": "^2.0.0",
    "@types/ws": "^8.5.3",
    "bumpp": "^8.2.1",
    "eslint": "^8.20.0",
    "esno": "^0.16.3",
    "pnpm": "^7.5.2",
    "typescript": "^4.7.4",
    "unbuild": "^0.7.4",
    "vitest": "^0.18.1"
  }
}
