{
  "name": "tiny-bilibili-ws",
  "type": "module",
  "version": "0.0.7",
  "packageManager": "pnpm@7.5.2",
  "description": "Bilibili Live WebSocket/TCP API",
  "author": "starknt",
  "license": "MIT",
  "homepage": "https://github.com/starknt/tiny-bilibili-ws#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/starknt/tiny-bilibili-ws.git"
  },
  "bugs": "https://github.com/starknt/tiny-bilibili-ws/issues",
  "keywords": [
    "ws",
    "websocket",
    "bilibili",
    "live"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./browser": {
      "types": "./dist/browser.d.ts",
      "require": "./dist/browser.cjs",
      "import": "./dist/browser.mjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "browser.d.ts"
  ],
  "scripts": {
    "build": "unbuild",
    "prepublishOnly": "pnpm run build",
    "release": "pnpm publish",
    "start": "esno ./src/node.ts",
    "dev": "unbuild --stub",
    "test": "vitest",
    "lint": "eslint ."
  },
  "dependencies": {
    "eventemitter3": "^4.0.7",
    "pako": "^2.0.4",
    "ws": "^8.10.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.29.3",
    "@types/node": "^18.11.9",
    "@types/pako": "^2.0.0",
    "@types/ws": "^8.5.3",
    "bumpp": "^8.2.1",
    "eslint": "^8.26.0",
    "esno": "^0.16.3",
    "pnpm": "^7.14.2",
    "typescript": "^4.8.4",
    "unbuild": "^0.9.4",
    "vitest": "^0.24.5"
  }
}
